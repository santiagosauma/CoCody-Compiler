lista <- [5, 0, 2, 15, 1].
n <- length(lista).

ciclo i desde 0 hasta (n - 1) ejecutar
    ciclo j desde 0 hasta (n - i - 2) ejecutar
        si (lista[j] > lista[j + 1]) ENTONCES
            temp <- lista[j].
            lista[j] <- lista[j + 1].
            lista[j + 1] <- temp.
        FIN_SI
    fin_ciclo
fin_ciclo

ciclo k desde 0 hasta (n - 1) ejecutar
    muestra(lista[k]).
fin_ciclo